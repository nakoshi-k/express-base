<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%- include('../../views/tile/head') %>
  </head>
  <body>
  <%- include('../../views/tile/navigation') %>

<div class="wrap">
  <div class="container">
    <div class="row">
      <div class="column column-25">
          <%- include('./sidebar') %>
      </div>
      <div class="column column-75">
        <%- hlp.form.start( "task" , task , {"method" :"post" , "action" : "./" }); %>
          <div class="form-item">
            <label for="title">Title</label>
            <%- hlp.form.input( "title" , {type : "text"} ); %>
            
          </div>
          <div class="form-item">
            <label for="priod">Priod</label>
            <%- hlp.form.input( "priod" , {class : "w100 calendar"} ); %>
          </div>
          <%- hlp.form.submit( "送信" ); %>

        <%- hlp.form.end(); %>
      </div>
    </div>
  </div>
</div>

<%- include('../../views/tile/footer') %>
<%- include('../../views/tile/scripts') %>

  </body>
</html>

