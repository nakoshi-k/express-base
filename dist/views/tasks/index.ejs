<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%- include('../components/head-base') %>
  </head>
  <body>
 <%- include('../components/global-nav') %>






 <div class="content row">
    
        <div class="col col-3">
            サイドナビ
        </div>
    
        <div class="col col-9">
         <main id="main">
          <% tasks.forEach(function (task) { %>
            <div>
              <div><h4><a href="/tasks/<%= task.id %>"><%= task.title %></a></h4></div>
              <a href="/tasks/<%= task.id %>/edit"> Edit</a>
            </div>
          <% }); %>

          <%- pagination.render(page , {path: "/tasks/page/#"}); %>
         </main>
        </div>
    
</div>





 <%- include('../components/scripts') %>
  </body>
</html>
