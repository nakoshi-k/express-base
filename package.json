{
  "name": "p3",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "distViewFile": "cpx \"apps/**/views/**/*.ejs\" dist/apps -w",
    "distComponentsFile": "cpx \"public/components/**/*.*\" dist/public/components -w",
    "distModelFile": "cpx \"models/**/*.*\" dist/models -w",
    "distConfigFile": "cpx \"config/**/*.*\" dist/config -w",
    "distTagFile": "cpx \"apps/public/tags/**/*.*\" dist/apps/public/tags -w",
    "distTemplateFile": "cpx \"base/templates/**/*.*\" dist/base/templates -w",
    "distStylus": "stylus apps/public/stylesheets/style.styl -o dist/apps/public/stylesheets/style.css --include-css",
    "watchStylus": "chokidar \"apps/public/stylesheets/**/*.styl\" --command \"npm run distStylus\"",
    "distComponetsScripts": "watchify apps/public/scripts/components.ts -p [tsify] -o dist/apps/public/scripts/components.js -v",
    "tsc": "tsc -w",
    "watch": "npm-run-all -p tsc distViewFile distComponentsFile distTagFile distConfigFile distModelFile watchStylus distComponetsScripts distTemplateFile",
    "test": "mocha ./dist/test/**/*.js"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "chai-http": "^3.0.0",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "deep-assign": "^2.0.0",
    "ejs": "~2.4.1",
    "express": "^4.13.4",
    "express-session": "^1.15.3",
    "flatpickr": "^3.0.7",
    "jquery": "^3.2.1",
    "method-override": "^2.3.9",
    "milligram-stylus": "^1.3.0",
    "morgan": "^1.7.0",
    "mysql": "^2.14.1",
    "mysql2": "^1.4.2",
    "node-uuid": "^1.4.8",
    "reflect-metadata": "^0.1.10",
    "riot": "^3.7.0",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.8.0",
    "serve-favicon": "^2.3.2",
    "tsc": "^1.20150623.0",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.37",
    "@types/mocha": "^2.2.43",
    "@types/node": "^7.0.43",
    "@types/sequelize": "^4.0.73",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "chai": "^4.1.2",
    "debug": "^2.2.0",
    "ejs": "^2.4.2",
    "mocha": "^3.5.3",
    "morgan": "^1.8.2",
    "tsify": "^3.0.3",
    "typescript": "^2.5.2"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.min.js",
    "confirmDatePlugin": "./node_modules/flatpickr/dist/plugins/confirmDate/confirmDate.js",
    "flatpickr": "./node_modules/flatpickr/dist/flatpickr.min.js"
  },
  "browserify-shim": {
    "jquery": "$"
  }
}
