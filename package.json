{
  "name": "p3",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "distViewFile": "cpx \"views/**/*.ejs\" dist/views/ -w",
    "distComponentsFile": "cpx \"public/components/**/*.*\" dist/public/components -w",
    "distModelFile": "cpx \"models/**/*.*\" dist/models -w",
    "distConfigFile": "cpx \"config/**/*.*\" dist/config -w",
    "distTagFile": "cpx \"public/tags/**/*.*\" dist/public/tags -w",
    "distStylus": "stylus public/stylus/style.styl -o dist/public/css/style.css --include-css",
    "watchStylus": "chokidar \"public/stylus/**/*.styl\" --command \"npm run distStylus\"",
    "tsc": "tsc -w",
    "watch": "npm-run-all -p tsc distViewFile distComponentsFile distTagFile distConfigFile distModelFile watchStylus",
    "test": "mocha ./dist/test/**/*.js"
  },
  "dependencies": {
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.37",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.18",
    "@types/sequelize": "^4.0.73",
    "body-parser": "^1.15.2",
    "chai-http": "^3.0.0",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "debug": "~2.2.0",
    "deep-assign": "^2.0.0",
    "ejs": "~2.4.1",
    "express": "^4.13.4",
    "express-session": "^1.15.3",
    "morgan": "^1.7.0",
    "mysql": "^2.14.1",
    "mysql2": "^1.4.2",
    "reflect-metadata": "^0.1.10",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.8.0",
    "serve-favicon": "^2.3.2",
    "tsc": "^1.20150623.0"
  },
  "devDependencies": {}
}
