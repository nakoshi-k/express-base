{
  "name": "p3",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "distViewFile": "cpx \"views/**/*.ejs\" dist/views/ -w",
    "distComponentsFile": "cpx \"public/components/**/*.*\" dist/public/components -w",
    "distStylus" : "stylus public/stylus/style.styl -o dist/public/css/style.css --include-css",
    "watchStylus" : "chokidar \"public/stylus/**/*.styl\" --command \"npm run distStylus\"",
    "tsc": "tsc -w",
    "watch": "npm-run-all -p tsc distViewFile distComponentsFile watchStylus"
  },
  "dependencies": {
    "@types/node": "^7.0.18",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.4.1",
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "serve-favicon": "^2.3.2"
  },
  "devDependencies": {}
}
