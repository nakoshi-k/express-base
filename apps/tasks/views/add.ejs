    <div class="row">
      <div class="column column-25">
          <%- include('./sidebar') %>
      </div>
      <div class="column column-75">

        <%- hlp.form.start( "task" , task , { "id" : "add" , "method" :"post" , "action" : "./" }); %>
        <fieldset>
        
          
          
          <div class="form-item">
            <label for="title">title</label>
            <%- hlp.form.input("title" , { "type" : "text" }); %>
          </div>
        
          
          <div class="form-item">
            <label for="title">priod</label>
            <%- hlp.form.input("priod" , { "class" : "calendar","type" : "text" }); %>
          </div>
        
          
          

        </fieldset>

        <%- hlp.form.submit( "submit" , { "class" : "xhr-post" , "data-target" : "#add" } ); %>
        <%- hlp.form.end(); %>
      </div>
    </div>
  </div>
</div>

